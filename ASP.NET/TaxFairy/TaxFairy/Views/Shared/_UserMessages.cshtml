@using TaxFairy.Core.Constants

@if (ViewData[MessageConstant.ErrorMessage]!=null)
{
    <script>
        $(function(){
        toastr.error('@ViewData[MessageConstant.ErrorMessage].ToString(), "", {positionClass: 'toast-bottom-right')
        })
    </script>
}     
@if (ViewData[MessageConstant.WarningMessage]!=null)
{
    <script>
        $(function(){
        toastr.warning('@ViewData[MessageConstant.WarningMessage].ToString(), "", {positionClass: 'toast-bottom-right')
        })
    </script>
}     
@if (ViewData[MessageConstant.SuccessMessage]!=null)
{
    <script>
        $(function(){
        toastr.success('@ViewData[MessageConstant.SuccessMessage].ToString()', "", {positionClass: 'toast-bottom-right'})
        })
    </script>
}     